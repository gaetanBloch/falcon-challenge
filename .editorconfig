# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.{ts,tsx,js,jsx,json,css,scss,yml,html,xml,vue}]
indent_size = 2
quote_type = single

[*.md]
max_line_length = off
trim_trailing_whitespace = false
